type Type {
  IntType()
  BoolType()
  UnitType()
}

type TypingType {
  TInt()
  TBool()
  TUnit()
  TVar(id: Int)
}

type Value {
  VInt(value: Int)
  VBool(value: Bool)
  VUnit()
}

type Address = Int

record TypeBinding(tpe: TypingType, isMutable: Bool)
record ValueBinding(value: Value, isMutable: Bool)
record Position (line: Int, column: Int)